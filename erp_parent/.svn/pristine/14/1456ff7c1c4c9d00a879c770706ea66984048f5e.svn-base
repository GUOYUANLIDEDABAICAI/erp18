<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="cn.itcast.erp.entity.Job" table="job" >
        <id name="uuid" >
            <generator class="org.hibernate.id.enhanced.SequenceStyleGenerator"> <param name="sequence_name">job_seq</param> </generator>
        </id>

        <property name="name" /> 
        <property name="suffix" /> 
        <property name="rank" /> 
        <property name="depuuid" /> 
        <property name="empuuid" /> 
        <property name="remark" /> 
        <property name="pid" /> 
 
    </class>
</hibernate-mapping>
